<?xml version="1.0" ?>
<serializer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns="http://symfony.com/schema/dic/serializer-mapping"
            xsi:schemaLocation="http://symfony.com/schema/dic/serializer-mapping https://symfony.com/schema/dic/serializer-mapping/serializer-mapping-1.0.xsd"
>
    <class name="Setono\SyliusFacebookPlugin\DTO\AbstractEvent">
        <attribute name="eventId" serialized-name="event_id">
            <group>setono:sylius-facebook:event</group>
        </attribute>

        <attribute name="eventName" serialized-name="event_name">
            <group>setono:sylius-facebook:event</group>
        </attribute>

        <attribute name="eventTime" serialized-name="event_time">
            <group>setono:sylius-facebook:event</group>
        </attribute>

        <attribute name="eventSourceUrl" serialized-name="event_source_url">
            <group>setono:sylius-facebook:event</group>
        </attribute>

        <attribute name="actionSource" serialized-name="action_source">
            <group>setono:sylius-facebook:event</group>
        </attribute>

        <attribute name="userData" serialized-name="user_data">
            <group>setono:sylius-facebook:event</group>
        </attribute>
    </class>
</serializer>
